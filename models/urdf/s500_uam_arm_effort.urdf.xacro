<?xml version="1.0"?>
<robot name="s500_uam_arm" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Links -->
  <link name="world"/>
  <joint name="world_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>


  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.772"/>
      <inertia ixx="0.0165" ixy="0" ixz="0"
              iyy="0.0186" iyz="0" izz="0.0250"/>
    </inertial>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="model://s500_uam/meshes/base_link.STL" scale="1 1 1"/>
      </geometry>
      <material name="DarkGrey"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.47 0.47 0.35"/>
      </geometry>
    </collision>
  </link>

  <link name="link_1">
    <inertial>
      <origin xyz="-0 -0.000196 -0.076809" rpy="0 0 0"/>
      <mass value="0.095"/>
      <inertia ixx="5.8e-05" ixy="0" ixz="0"
               iyy="5.1e-05" iyz="0" izz="1.8e-05"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="model://s500_uam/meshes/link_1.STL" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="model://s500_uam/meshes/link_1.STL" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint_1" type="revolute">
    <origin xyz="0.0007 0 -0.00275" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="link_1"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="1" velocity="1"/>
    <dynamics>
      <damping>0.05</damping>
      <friction>0.1</friction>
    </dynamics>
  </joint>

  <!-- Link 2 -->
  <link name="link_2">
    <inertial>
      <origin xyz="0.000575 0.000247 -0.072261" rpy="0 0 0"/>
      <mass value="0.082"/>
      <inertia ixx="4.3e-05" ixy="0" ixz="0"
               iyy="4.1e-05" iyz="0" izz="2.6e-05"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="model://s500_uam/meshes/link_2.STL" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="model://s500_uam/meshes/link_2.STL" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint_2" type="revolute">
    <origin xyz="0 0 -0.09925" rpy="0 0 0"/>
    <parent link="link_1"/>
    <child link="link_2"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="1" velocity="1"/>
    <dynamics>
      <damping>0.05</damping>
      <friction>0.1</friction>
    </dynamics>
  </joint>

  <!-- Link 3 (Gripper Left Finger) -->
  <link name="link_3">
    <inertial>
      <origin xyz="0.025274 0.000991 -0.041284" rpy="0 0 0"/>
      <mass value="0.012"/>
      <inertia ixx="1.0e-05" ixy="0" ixz="0"
               iyy="1.0e-05" iyz="0" izz="2.0e-06"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="model://s500_uam/meshes/link_3.STL" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="model://s500_uam/meshes/link_3.STL" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint_3" type="revolute">
    <origin xyz="-0.011 0 -0.08225" rpy="0 0.23432 -3.14159"/>
    <parent link="link_2"/>
    <child link="link_3"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="0.5" velocity="0.5"/>
  </joint>

  <!-- Link 4 (Gripper Right Finger - mimic) -->
  <link name="link_4">
    <inertial>
      <origin xyz="0.024422 -0.000991 -0.039818" rpy="0 0 0"/>
      <mass value="0.012"/>
      <inertia ixx="1.0e-05" ixy="0" ixz="0"
               iyy="1.0e-05" iyz="0" izz="2.0e-06"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="model://s500_uam/meshes/link_4.STL" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="model://s500_uam/meshes/link_4.STL" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint_4" type="revolute">
    <origin xyz="0.011 0 -0.08225" rpy="0 0.26237 0"/>
    <parent link="link_2"/>
    <child link="link_4"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="1.0" velocity="0.5"/>
  </joint>

